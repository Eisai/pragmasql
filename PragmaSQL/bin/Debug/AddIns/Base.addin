<AddIn name        = "PragmaSQL Editor"
       author      = "Ali Özgür"
       url         = ""
       description = "T-SQL Editor for MS SQL Server 2000/2005"
       addInManagerHidden = "true">
  
  <Runtime>
    <Import assembly = "../PragmaSQL.exe"/>
  </Runtime>

  <Path name = "/Workspace/HelpMenu"/>
  <Path name = "/Workspace/ToolsMenu"/>
  <Path name = "/Workspace/Toolbar"/>
  
  <Path name = "/Workspace/ProjectExplorer/Toolbar"/>
  <Path name = "/Workspace/ProjectExplorer/ContextMenu"/>
  
  <Path name = "/Workspace/ScriptEditor/Toolbar"/>
  <Path name = "/Workspace/ScriptEditor/ContextMenu"/>
  <Path name = "/Workspace/ScriptEditor/MainMenu"/>
  
  <Path name = "/Workspace/ScriptEditor/ContentContextMenu"/>
  <Path name = "/Workspace/ScriptEditor/ResultsContextMenu"/>
  <Path name = "/Workspace/ScriptEditor/MessagesContextMenu"/>

  <Path name = "/Workspace/TextEditor/Toolbar"/>
  <Path name = "/Workspace/TextEditor/ContextMenu"/>
  <Path name = "/Workspace/TextEditor/MainMenu"/>
  <Path name = "/Workspace/TextEditor/ContentContextMenu"/>

  <Path name = "/Workspace/ObjectExplorer/ContextMenu"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/Folder"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/Server"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/Database"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/UserTable"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/SystemTable"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/View"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/Trigger"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/StoredProcedure"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/ScalarValuedFunction"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/TableValuedFunction"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/UsersGroup"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/Column"/>
  <Path name = "/Workspace/ObjectExplorer/ContextMenu/Synonym"/>
  

  <Path name = "/Workspace/ObjectExplorer/Toolbar"/>

  <Path name = "/Workspace/ProjectExplorer/ContextMenu"/>
  <Path name = "/Workspace/ProjectExplorer/ContextMenu/Project"/>
  <Path name = "/Workspace/ProjectExplorer/ContextMenu/ConnectionSpec"/>
  <Path name = "/Workspace/ProjectExplorer/ContextMenu/Folder"/>
  <Path name = "/Workspace/ProjectExplorer/ContextMenu/ScriptableDBObject"/>
  <Path name = "/Workspace/ProjectExplorer/ContextMenu/NonScriptableDBObject"/>
  <Path name = "/Workspace/ProjectExplorer/ContextMenu/ScriptFile"/>
  <Path name = "/Workspace/ProjectExplorer/ContextMenu/TextFile"/>
  <Path name = "/Workspace/ProjectExplorer/ContextMenu/ExternalFile"/>
  <Path name = "/Workspace/ProjectExplorer/Toolbar"/>

  <Path name = "/Workspace/SharedScriptsViewer/Toolbar"/>
  <Path name = "/Workspace/SharedScriptsViewer/ContextMenu"/>

  <Path name = "/Workspace/SharedSnippetsViewer/Toolbar"/>
  <Path name = "/Workspace/SharedSnippetsViewer/ContextMenu"/>
</AddIn>
