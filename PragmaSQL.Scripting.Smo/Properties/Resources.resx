  <data name="Script_ObjectMatch" xml:space="preserve">
    <value>SELECT
	id,
	[name]
	, CASE xtype 
		WHEN 'U' THEN 1
		WHEN 'FN' THEN 2
		WHEN 'TF' THEN 2
		WHEN 'IF' THEN 2
		WHEN 'P' THEN 3
		WHEN 'X' THEN 4
		WHEN 'TR' THEN 5
	  ELSE 1000 END atype
FROM sysobjects ORDER BY atype,[name]

WHERE LOWER(NAME) IN ({0})</value>
  </data>
  <data name="database_connect" type="System.Resources.ResXFileRef, System.Windows.Forms">
    <value>..\Resources\database_connect.png;System.Drawing.Bitmap, System.Drawing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a</value>
  </data>
  <data name="Script_ColumnMap" xml:space="preserve">
    <value>SELECT 
	sc.name
	, COLUMNPROPERTY( OBJECT_ID('{0}.{1}'),sc.name,'IsComputed')AS 'IsComputed' 
FROM  syscolumns sc 
JOIN sysobjects so on sc.id = so.id
JOIN sysusers su on so.uid = su.uid
WHERE su.name = '{2}' AND so.name = '{3}'</value>
  </data>
