<AddIn name        = "PragmaSQL File Explorer"
       author      = "Ali Özgür"
       description = "File Explorer">
	<Manifest>
		<Identity name = "PragmaSQL.FileExplorer"/>
	</Manifest>

	<Runtime>
		<Import assembly="PragmaSQL.FileExplorer.dll"/>
	</Runtime>


	<Path name = "/Workspace/ToolsMenu">
		<MenuItem id = "FileExplorer.Run"
						 label = "File Explorer"
						 class ="PragmaSQL.FileExplorer.RunExplorer"/>
	</Path>
</AddIn>
